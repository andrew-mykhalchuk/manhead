block vars
	-var current = 'Home'; //default


-var nav = { 'Home': '/', 'Archive': '/archive.html', 'Browse Data': '/browse-data-card-view.html', 'Manage Contacts': '/manage-contacts.html' };

header
	a.logo(href="./")
		img(src="../images/manhead-logo.svg", alt="")

	nav.main-nav
		ul
			each val, key in nav
				if current === key
					li.active
						a(href=val, title=key)= key
				else
					li
						a(href=val, title=key)= key